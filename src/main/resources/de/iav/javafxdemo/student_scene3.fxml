<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<FlowPane prefHeight="539.0" prefWidth="614.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.iav.studentwithscenes.StudentScene3Controller">
   <children>
      <VBox prefHeight="539.0" prefWidth="615.0" spacing="20.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="List of all students" textAlignment="CENTER">
               <font>
                  <Font size="22.0" />
               </font>
            </Text>
            <ListView fx:id="students" prefHeight="200.0" prefWidth="200.0" />
            <HBox prefHeight="20.0" prefWidth="615.0">
               <children>
                  <TextField fx:id="studentFirstNameTF" prefHeight="25.0" prefWidth="247.0" />
                  <TextField fx:id="studentSurnameTF" prefHeight="25.0" prefWidth="262.0" />
                  <TextField fx:id="studentEmailTF" prefHeight="25.0" prefWidth="271.0" />
                  <TextField fx:id="studentCourseTF" prefHeight="25.0" prefWidth="264.0" />
               </children>
            </HBox>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
               <items>
                  <Button fx:id="addStudentButton" mnemonicParsing="false" text="Add student" onAction="#onAddButton" />
               </items>
            </ToolBar>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                  <Button fx:id="switchToSceneOneButton" mnemonicParsing="false" onAction="#onSSButton_switchToScene1" text="To registration" />
              </items>
            </ToolBar>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" />
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </VBox>
   </children>
</FlowPane>
